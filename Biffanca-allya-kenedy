import requests
from bs4 import BeautifulSoup

def scrape_carsome():
    url = "https://www.carsome.my"
    response = requests.get(url)
    if response.status_code == 200:
        soup = BeautifulSoup(response.content, 'html.parser')
        # Lakukan parsing halaman web dan ambil data kendaraan yang diinginkan
        # Contoh: nama kendaraan, harga, tahun pembuatan, dll.
        # Selanjutnya, Anda dapat menyimpan data ini ke dalam struktur data seperti daftar atau DataFrame.

